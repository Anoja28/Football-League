<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="teamRoot" prefWidth="${teamRoot.parent.width}"
    xmlns="http://javafx.com/javafx/8.0.171" VBox.vgrow="always"
    xmlns:fx="http://javafx.com/fxml/1" spacing="0">
    <HBox alignment="baseline_left" fx:id="teamPageHeader">
        <Label prefWidth="140" alignment="center" fx:id="logoContainer" />
        <Label styleClass="section-header" fx:id="teamName" />
    </HBox>
    <VBox fx:id="content" style="-fx-padding: 30; -fx-background-color: white;" HBox.hgrow="always" />
</VBox>